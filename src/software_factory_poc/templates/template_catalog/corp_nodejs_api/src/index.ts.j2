import express from 'express';

const app = express();
const port = 3000;

app.get('/', (req, res) => {
  res.send('Hello from {{ service_name }}!');
});

app.get('/health', (req, res) => {
  res.json({ status: 'ok', owner: '{{ owner_team }}' });
});

app.listen(port, () => {
  console.log(`{{ service_name }} listening at http://localhost:${port}`);
});
